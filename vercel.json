{
    "builds": [
      {
        "src": "api/extract_recipe.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/extract",
        "dest": "/api/extract_recipe.py",
        "methods": ["GET"],
        "headers": {
          "x-api-key": "$EXTRACTOR_API_KEY"
        }
      }
    ],
    "env": {
      "VERCEL_PYTHON_RUNTIME": "python3.9"
    }
  }